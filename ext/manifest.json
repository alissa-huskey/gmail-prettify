{
  "name": "New Gmail Less Ugly",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "CSS changes to the new gmail including: change the compose button, change the background of the Inbox label, remove the hangouts chat interface.",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon-16.png",
    "19": "icons/icon-19.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
